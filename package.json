{
  "name": "tech-challenge-3",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext .js,.jsx",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "docker:build": "docker build -t tech-challenge-3 .",
    "docker:build-dev": "docker build -f Dockerfile.dev -t tech-challenge-3-dev .",
    "docker:run": "docker run -p 8080:80 tech-challenge-3",
    "docker:run-dev": "docker run -p 3000:3000 -v $(pwd):/app -v /app/node_modules tech-challenge-3-dev",
    "docker:dev": "docker compose up app-dev -d",
    "docker:prod": "docker compose up app-prod -d",
    "docker:stop": "docker compose down",
    "docker:clean": "docker compose down --rmi all --volumes"
  },
  "dependencies": {
    "@mdi/js": "^7.4.47",
    "@mdi/react": "^1.6.1",
    "axios": "^1.11.0",
    "dompurify": "^3.2.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-hot-toast": "^2.6.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.8.2",
    "styled-components": "^6.1.19",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  }
}
